{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"],
			"$services": ["src/services/*"],
			"$services/*": ["src/services/*"],
			"$stores": ["src/stores/*"],
			"$stores/*": ["src/stores/*"],
			"$utils": ["src/utils/*"],
			"$utils/*": ["src/utils/*"]
		}
	},
	"include": ["src/**/*.js", "src/**/*.svelte"]
}
